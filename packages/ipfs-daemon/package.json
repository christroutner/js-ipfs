{
  "name": "@chris.troutner/ipfs-daemon",
  "version": "0.8.0",
  "description": "JavaScript implementation of the IPFS specification",
  "keywords": [
    "IPFS"
  ],
  "homepage": "https://github.com/christroutner/js-ipfs/tree/master/packages/ipfs-daemon#readme",
  "bugs": "https://github.com/christroutner/js-ipfs/issues",
  "license": "(Apache-2.0 OR MIT)",
  "leadMaintainer": "Alex Potsides <alex@achingbrain.net>",
  "files": [
    "src",
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "main": "src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christroutner/js-ipfs.git"
  },
  "scripts": {
    "lint": "aegir lint",
    "test": "npm run test:node",
    "test:node": "aegir test -t node -- --exit",
    "coverage": "nyc --reporter=text --reporter=lcov npm run test:node",
    "clean": "rimraf ./dist",
    "dep-check": "aegir dep-check -i ipfs-core-types -i @mapbox/node-pre-gyp",
    "build": "aegir build --no-bundle"
  },
  "dependencies": {
    "@chris.troutner/ipfs-core": "^0.9.0",
    "@chris.troutner/ipfs-core-types": "^0.6.0",
    "@chris.troutner/ipfs-grpc-server": "^0.4.0",
    "@chris.troutner/ipfs-http-client": "^51.0.0",
    "@chris.troutner/ipfs-http-gateway": "^0.5.0",
    "@chris.troutner/ipfs-http-server": "^0.6.0",
    "@mapbox/node-pre-gyp": "^1.0.5",
    "debug": "^4.1.1",
    "dlv": "^1.1.3",
    "ipfs-utils": "^8.1.2",
    "just-safe-set": "^2.2.1",
    "libp2p": "^0.32.4",
    "libp2p-delegated-content-routing": "^0.10.0",
    "libp2p-delegated-peer-routing": "^0.9.0",
    "libp2p-webrtc-star": "^0.22.2",
    "multiaddr": "^9.0.1"
  },
  "devDependencies": {
    "aegir": "^33.0.0",
    "node-fetch": "npm:@achingbrain/node-fetch@^2.6.4",
    "ws": "^7.3.1"
  },
  "optionalDependencies": {
    "electron-webrtc": "^0.3.0",
    "prom-client": "^12.0.0",
    "prometheus-gc-stats": "^0.6.0",
    "wrtc": "^0.4.6"
  }
}
